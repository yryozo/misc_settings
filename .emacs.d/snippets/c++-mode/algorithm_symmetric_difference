// WARNING: 2 containers must be sorted!
${1:vector}< remove_reference<decltype(*begin($3))>::type > ${2:vv3}; // 結果
std::set_symmetric_difference(${3:vv1}.begin(), $3.end(), ${4:vv2}.begin(), $4.end(), back_inserter($2));
